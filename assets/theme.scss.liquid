.tabs .tab-label {
  display: inline-block;
  width: 100%;
  font-size: 1rem;
  position: relative;
  vertical-align: bottom;
  font-style: normal;
  font-family: 'Avenir Next Demi','Times New Roman', serif;
  span {
    padding: 18px 14px 19px;
    line-height: 2.4em;
    pointer-events: none;
    float: left;
    color: $colorBlack;
    font-family: $avenirNext;
    font-size: 1.1rem;
    font-weight: 500;
    letter-spacing: 1px;
    line-height: 1.5rem;
  }
}

.tabs .tab > [type="radio"] {
  clip: rect(0 0 0 0);
  height: 1px;
  opacity: 0;
  position: fixed;
  width: 1px;
  z-index: -1;
  &:checked + label {
    .down {
      display: none;
    }
    .up {
      display: block;
    }
  }

  + label {
    .icon {
      color: $colorPink;
      line-height: 1.3rem;
      font-size: 1.3rem;
    }
    .down {
      display: block;
    }
    .up {
      display: none;
    }
  }
}

.tabs .tab-panel {
  display: inline;
  display: inline-block;
  overflow: hidden;
  height: 0;
  width: 0;
}

.tabs .tab-content {
  position: absolute;
  max-height: 0;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  display: block;
  padding: 0;
  float: left;
  font-size: 16px;
  font-size: 1rem;
  margin-top: -1px;
  width: 100%;
  .product__for-whom {
    > li {
      padding-right: 24px;
      padding-left: 24px;
    }
    > li {
      &:not(:last-child) {
        border-right: 1px solid #dddee0;
      }
    }
    @include respond-to('small'){
      > li {
        padding-right: 5px;
        padding-left: 5px;
      }
    }
  }
}

.tabs .tab [type="radio"]:not(:checked) + .tab-label {
  i.arrow.up {
    display: none;
  }

}

.tabs .tab [type="radio"]:checked + .tab-label {
  background: white;
  z-index: 1;
  @include respond-to('medium'){
    i.arrow.down {
      display: none;
    }
    i.arrow.up {
      display: block;
    }
  }
}

.tabs .tab [type="radio"]:checked ~ .tab-panel {
  display: inline;
  .tab-content{
    position: relative;
    max-height: 4000px;
    padding: 20px;
    transition: max-height 4s;
    background-color: #ffffff;
  }
  .consumer-study-chart{
    circle{
      animation: fillup 0.8s ease-out forwards;
    }
  }
}


/*

=======================================
INTERNET EXPLORER 8 and below
=======================================

*/
@media \0screen\,screen\9 {
  /* IE7 and IE6 */
  .tabs {
    zoom: 1;
  }

  /* IE8 and below */
  .tabs .tab .tab-radio {
    filter: Alpha(opacity=0);
    position: absolute;
    z-index: -1;
  }

  /* IE7 and IE6 */
  .tabs .tab .tab-panel, {
    display: none;
    height: auto;
    overflow: visible;
    position: static;
    width: auto;
  }

  /* IE7 and IE6 */
  .tabs .tab-content, {
    float: none;
    padding-bottom: 1px;
    padding-top: 1px;
    width: auto;
  }

  /* IE8 and below */
  .tabs .checked label {
    background: white;
    border-bottom-width: 0;
    z-index: 1;
  }

  /* IE8 */
  .tabs .checked .tab-panel {
    display: inline;
  }
  /* IE7 and IE6 */
  .tabs .checked .tab-panel, {
    display: inline;
    float: left;
    width: 100%;
  }
}

/*

=======================================
UNIMPORTANT STYLES BEGIN HERE
=======================================

*/


.tabs {
  color: #222;
}

.tabs a:link,.tabs a:visited,.tabs a:hover,.tabs a:focus,.tabs a:active {
  color: $colorPurityDark;
  border-bottom: 0.08em solid rgba(0,0,0,0.1)
}


.for {
  display: inline-block;
  position: relative;
}

.for:after {
  background: #DDD;
  bottom: 150%;
  color: #222;
  font-family: sans-serif;
  font-size: 12px;
  opacity: 0;
  padding: .5em;
  pointer-events: none;
  position: absolute;
  right: 0;
  -webkit-transition: opacity .2s ease-in-out;
  transition: opacity .2s ease-in-out;
  white-space: nowrap;
}

.for:hover:after {
  opacity: 1;
}

.for.default-tab {
  color: #8AD;
}

.for.ie8-and-below {
  color: #DA8;
}

.for.default-tab:after {
  content: 'For default tab';
}

.for.ie8-and-below:after {
  content: 'For IE8 and below';
}

/* and just for fun... */
@media only screen and (max-width: 43em) {
  .tabs .tab > label {
    background: #f3f3f3;
    border-color: #dddddd;

    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    width: 100%;
  }
  .tabs .tab [type="radio"]:checked + .tab-label {
    // background: #222;
    // border-color: #444;
    color: $colorPurityDark;
  }
}



.tab i {
  border: solid $colorPurityDark;
  border-width: 0 3px 3px 0;
  display: inline-block;
  padding: 3px;
  &.white-arrow {
    border-color: $colorPurityWhite;
  }
}

i.right {
  transform: rotate(-45deg);
  -webkit-transform: rotate(-45deg);
}

i.left {
  transform: rotate(135deg);
  -webkit-transform: rotate(135deg);
}

i.up {
  transform: rotate(-135deg);
  -webkit-transform: rotate(-135deg);
}

i.down {
  transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
}
ul.icon-block {
  @include respond-to('small'){
    li {
      margin: 0;
      font-size: 9px;
      span {
        vertical-align: super;
      }
    }
  }
}


