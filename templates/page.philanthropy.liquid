<style media="screen">
  .charity-bg[data=dog]{
    background-image: url('https://cdn.no-toxic.com/image/fetch/q_auto:best,f_auto,fl_lossy,w_auto,c_limit,dpr_auto/https:{{ 'charity-desktop.jpg' | file_img_url: '2048x' | format: 'pjpg' }}');
  }
  .charity-bg[data=tree]{
    background-image: url('https://cdn.no-toxic.com/image/fetch/q_auto:best,f_auto,fl_lossy,w_auto,c_limit,dpr_auto/https:{{ 'charity-tree.png' | file_img_url: '2048x' | format: 'pjpg' }}');
  }
  .donate-cart-container{
    bottom: -150px;
  }
  .donate-cart-container svg{
    height: 80px;
  }
  .donate-card.active svg{
    height: 100px;
  }
  .donate-card{
    -webkit-box-shadow: 3px 2px 6px rgba(33,33,33,0.5);
    box-shadow: 3px 2px 6px rgba(33,33,33,0.5);
    border-radius: 10px;
  }
  .donate-card.active{
    transform: translateY(-7px) scale(1.1);
  }
  .dog-container{
    background-color: #E2B696;
    transform-origin: left;
  }
  .tree-container{
    background-color: #838A2E;
    transform-origin: right;
  }
  #select_tree,#select_dog{
    display: none;
    animation-iteration-count: infinite;
    animation-timing-function: linear;
  }
  .active #select_tree, .active #select_dog{
    display: block;
  }
  #select_tree{
    transform-origin: 16% 22%;
    animation-duration: 4s;
  }
  #select_dog{
    transform-origin: 50% 10%;
    animation-duration: 2s;
  }
  .donate-cart-container a:hover, .donate-cart-container a:focus{
    color: white;
  }
  a.tree-container:hover #select_tree{
    display: block;
    animation-name: tree-animation;
  }
  a.dog-container:hover #select_dog{
    display: block;
    animation-name: dog-animation;
  }
  .slider-container{
    width: 200%;
    transition: all 0.4s;
  }
  .donate-dog-section{
    background-color: rgba(252,228,222,0.1);
  }
  .donate-dog-section .same-height{
    border: solid 2px #F38188;
  }
  .donate-tree-section{
    background-color: rgba(183,194,150,0.1);
  }
  .donate-tree-section .same-height{
    border: solid 2px #4C914B;
  }
  .rotate-icon div{
    margin-top: 80%;
  }
  .donation-list{
    height: 4em;
    overflow: hidden;
  }
  @media (max-width: 769px){
    .charity-bg{
      background-image: url('https://cdn.no-toxic.com/image/fetch/q_auto:best,f_auto,fl_lossy,w_auto,c_limit,dpr_auto/https:{{ 'charity-mobile.jpg' | file_img_url: '2048x' | format: 'pjpg' }}');
    }
    .donate-number{
      font-size: 48px;
    }
    .rotate-icon{
      transform: rotate(90deg);
      margin-left: 10%;
    }
    .rotate-icon div{
      margin-top: 0%;
      margin-bottom: 60%;
    }
  }
  @keyframes tree-animation {
    from {transform: rotate(0deg);}
    to {transform: rotate(360deg);}
  }
  @keyframes dog-animation {
    0 {transform: rotate(0deg);}
    6.25%   {transform: rotate(10deg);}
    12.5%   {transform: rotate(0deg);}
    18.25%  {transform: rotate(-10deg);}
    25%     {transform: rotate(0deg);}
    31.25%  {transform: rotate(10deg);}
    37.5%   {transform: rotate(0deg);}
    43.25%  {transform: rotate(-10deg);}
    50%     {transform: rotate(0deg);}
    100%    {transform: rotate(0deg);}
  }
</style>
