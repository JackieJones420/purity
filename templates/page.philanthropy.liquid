<script type="text/javascript">
  $('.charity-show-more').click(()=>{
    $('.charity-show-more').addClass('hide');
    $('.donation-list').css('height','auto');
  })

  $('.donate-card').click(function(){
    $('.donate-card').removeClass('active');
    $(this).addClass('active');
    $('.slider-container').css('transform',`translateX(-${$(this).index()*50}%)`);
    $('.charity-bg').attr('data',$(this).attr('data'));
    history.pushState(null, '', `/pages/philanthropy/${$(this).attr('data')}`);
  })

  $(document).ready(function() {

    fetch('https://api.allorigins.win/get?url=' + encodeURIComponent('https://toolbox.puritycosmetics.com/services/dogtree/list.php'))
      .then(response => response.json())
      .then(result => {
        
        let data = result.contents;

        data = JSON.parse(data);
        data = data.resource;
                
        $('.dog-number').html(data[0].QTY); //dog
        $('.tree-number').html(data[1].QTY); //tree

        })
      .catch(error => console.log('error', error));
